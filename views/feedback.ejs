<%- include('header2'); -%>

    <!DOCTYPE html>
    <html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="uploadstyle.css">
        <title>Product Feedback</title>
    </head>

    <body>
        <div class="add-form">
            <h1>
                Submit Your Feedback
            </h1>
            <form action="ufeedback" method="POST">
                <label for="name" class="form-label">Name</label>
                <input type="text" name="name" id="name" autocomplete="off" required />
                <label for="email">Email</label><input type="email" id="email" name="email" required />
                <label for="star">Ratings (On scale of 5)</label><input type="number" name="star" id="star"
                    autocomplete="off" required />
                <label for="review">Reviews</label><input type="text" id="review" name="review" required />

                <!-- <label for="password">Password</label><input type="password" name="password" id="password" required
                value="<MD5(%=data.userPassword%)>"> -->


                <input type="hidden" name="usrid" id="usrid" value=<%=usrid%>>
                <input type="hidden" name="pid" id="pid" value="<%=pid%>">
                <div>
                    <input type="submit" name="ufeedback" id="ufeedback">
                </div>


            </form>
        </div>
    </body>

    </html>

    <%- include('footer'); -%>